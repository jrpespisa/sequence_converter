<!DOCTYPE html>
<html data-ng-app="converterApp">
<head>
  <title>ConverterApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%- flash.each do |key, value| -%>
  <div class="flash flash-<%= key %>">
    <%= value %>
  </div>
<%- end -%>

<%= yield %>

</body>
</html>
